{
  "compilerOptions": {
    "types": ["google-apps-script", "node"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": false,
    "sourceMap": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": false,
    "lib": ["es6", "dom"]
  },
  "exclude": ["node_modules", "dist"]
}

{
    "defaultSeverity": "error",
    "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "variable-name": false,
        "function-name": false,
        "jsx-no-lambda": false,
        "no-this-assignment": false,
        "import-name": false,
        "no-increment-decrement": false,
        "no-implicit-dependencies": false,
        "no-floating-promises": true
    },
    "rulesDirectory": [],
    "globals": {
        "CONFIG": true
    }
}
